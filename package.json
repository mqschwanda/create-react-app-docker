{
  "name": "create-react-app-docker",
  "version": "0.0.0-alpha",
  "private": true,
  "dependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "react-scripts": "./node_modules/.bin/react-scripts",
    "start:prod": "docker-compose down && docker-compose up prod",
    "start": "docker-compose down && docker-compose up dev",
    "stop": "docker-compose down",
    "test": "docker-compose down && docker-compose run --rm test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
